{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "gen:graphql": {
      "cache": false,
      "persistent": true
    },
    "deploy:start": {
      "cache": false,
      "persistent": true
    },
    "strapi": {
      "cache": false,
      "persistent": true
    }
  },
  "ui": "tui"
}