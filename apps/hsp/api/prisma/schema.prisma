generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
  binaryTargets = ["native", "debian-openssl-3.0.x", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("API_DATABASE_URL")
}

model test {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  name     String
}
